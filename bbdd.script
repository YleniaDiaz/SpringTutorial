USE demo;

CREATE TABLE IF NOT EXISTS person {
		id INT PRIMARY KEY NOT NULL AUTO_INCREMENT,
		name VARCHAR(255),	
		surname VARCHAR(255),	
		email VARCHAR(255) NOT NULL UNIQUE,	
		telephone VARCHAR(255)
};


